<?xml version="1.0" encoding="UTF-8"?>
<list id="local-relationships.xml" type="relationship"
      n="rif-cs 1.0.1 relationship types and display labels for relatedObjects"
      src="http://ands.org.au/guides/cpguide/cpgrelatedobject.html">
   <activity>

      <any>

         <item>
            <code>hasAssociationWith</code>
            <desc>Associated with</desc>
            <help>Has an unspecified relationship with the related activity</help>
         </item>

      </any>

      <activity>

         <item>
            <code>isPartOf</code>
            <desc>Part of</desc>
            <help>Is contained in the related activity</help>
         </item>

         <item>
            <code>hasPart</code>
            <desc>Includes</desc>
            <help>Contains the related activity</help>
         </item>

         <item>
            <code>isFundedBy</code>
            <desc>Funded by</desc>
            <help>Receives monetary or in-kind aid from the related party or program</help>
         </item>

      </activity>

      <collection>

         <item>
            <code>hasOutput</code>
            <desc>Produces</desc>
            <help>Delivers materials in the related collection</help>
         </item>

      </collection>

      <party>

         <item>
            <code>isFundedBy</code>
            <desc>Funded by</desc>
            <help>Receives monetary or in-kind aid from the related party or program</help>
         </item>

         <item>
            <code>isManagedBy</code>
            <desc>Managed by</desc>
            <help>Is organised and/or delivered by the related party (includes custodian role)</help>
         </item>

         <item>
            <code>isOwnedBy</code>
            <desc>Owned by</desc>
            <help>Legally belongs to the related part</help>
         </item>

         <item>
            <code>hasParticipant</code>
            <desc>Undertaken by</desc>
            <help>Is undertaken by the related party</help>
         </item>

      </party>

   </activity>

   <collection>

      <collection>
         <item>
            <code>describes</code>
            <desc>Describes</desc>
            <help>Is a catalogue for, or index of, items in a related collection</help>
         </item>

         <item>
            <code>hasPart</code>
            <desc>Includes</desc>
            <help>Contains the related collection</help>
         </item>

         <item>
            <code>hasAssociationWith</code>
            <desc>Associated with</desc>
            <help>Has an unspecified relationship with the related collection</help>
         </item>

         <item>
            <code>isDescribedBy</code>
            <desc>Described by</desc>
            <help>Is catalogued or indexed by the related collection</help>
         </item>
         <item>
            <code>isLocatedIn</code>
            <desc>Located in</desc>
            <help>Is held in the related repository</help>
         </item>
         <item>
            <code>isLocationFor</code>
            <desc>Location for</desc>
            <help>Is the repository where the related collection is held</help>
         </item>

         <item>
            <code>isPartOf</code>
            <desc>Part of</desc>
            <help>Is contained within the related collection</help>
         </item>

         <item>
            <code>isDerivedFrom</code>
            <desc>Derived from</desc>
            <help>Collection is derived from the related collection e.g. through analysis</help>
         </item>

         <item>
            <code>hasDerivedCollection</code>
            <desc>Has derived</desc>
            <help>The related collection is derived from the collection e.g. through analysis</help>
         </item>

      </collection>

      <party>
         <item>
            <code>hasCollector</code>
            <desc>Aggregated by</desc>
            <help>Has been aggregated by the related part</help>
         </item>

         <item>
            <code>isManagedBy</code>
            <desc>Managed by</desc>
            <help>Is maintained and made accessible by the related party (includes custodian role)</help>
         </item>

         <item>
            <code>isOwnedBy</code>
            <desc>Owned by</desc>
            <help>Legally belongs to the related party</help>
         </item>

         <item>
            <code>isEnrichedBy</code>
            <desc>Enriched by</desc>
            <help>Additional value provided to a collection by a party</help>
         </item>


      </party>

      <activity>
         <item>
            <code>isOutputOf</code>
            <desc>Output of</desc>
            <help>Is a product of the related activity</help>
         </item>

      </activity>

      <service>
         <item>
            <code>supports</code>
            <desc>Supports</desc>
            <help>Can be contributed to, accessed or used through the related service (in technical terms, the collection supports the service)</help>
         </item>

         <item>
            <code>isAvailableThrough</code>
            <desc>Available through</desc>
            <help>Discovery Services (Harvest, Search, Syndicate)</help>
         </item>

         <item>
            <code>isProducedBy</code>
            <desc>Produced by</desc>
            <help>Creation Services (Create, Generate, Assemble, Transform output)</help>
         </item>

         <item>
            <code>isPresentedBy</code>
            <desc>Presented by</desc>
            <help>Creation Services (Report)</help>
         </item>

         <item>
            <code>isOperatedOnBy</code>
            <desc>Operated on by</desc>
            <help>Creation Services (Transform input)</help>
         </item>

         <item>
            <code>hasValueAddedBy</code>
            <desc>Value added by</desc>
            <help>Metadata Services (Annotate, Classify)</help>
         </item>

      </service>

   </collection>

   <party>
      <party>
         <item>
            <code>hasMember</code>
            <desc>Has member</desc>
            <help>(group only) has enrolled the related party in the group</help>
         </item>

         <item>
            <code>hasPart</code>
            <desc>Includes</desc>
            <help>(group only) contains the related group</help>
         </item>

         <item>
            <code>isPartOf</code>
            <desc>Part of</desc>
            <help>(group only) is contained in the related group</help>
         </item>

         <item>
            <code>isMemberOf</code>
            <desc>Member of</desc>
            <help>Is enrolled in the related group</help>
         </item>

         <item>
            <code>isFundedBy</code>
            <desc>Funded by</desc>
            <help>Receives monetary or in-kind aid from the related party or program</help>
         </item>
         <item>
            <code>isFunderOf</code>
            <desc>Funds</desc>
            <help>Provides monetary or in-kind aid to the related party or activity</help>
         </item>
      </party>

      <collection>

         <item>
            <code>enriches</code>
            <desc>Enriches</desc>
            <help>(collections only) provides additional value to a collection</help>
         </item>

         <item>
            <code>isCollectorOf</code>
            <desc>Collector of</desc>
            <help>Has aggregated the related collection</help>
         </item>

      </collection>

      <activity>

         <item>
            <code>isParticipantIn</code>
            <desc>Participant in</desc>
            <help>Is enrolled in the related activity</help>
         </item>

         <item>
            <code>isFundedBy</code>
            <desc>Funded by</desc>
            <help>Receives monetary or in-kind aid from the related program</help>
         </item>

         <item>
            <code>isFunderOf</code>
            <desc>Funds</desc>
            <help>Provides monetary or in-kind aid to the related activity</help>
         </item>

      </activity>

      <any>

         <item>
            <code>hasAssociationWith</code>
            <desc>Associated with</desc>
            <help>Has an unspecified relationship with the related registry object</help>
         </item>

         <item>
            <code>isManagedBy</code>
            <desc>Managed by</desc>
            <help>Is overseen by the related party (includes custodian role)</help>
         </item>

         <item>
            <code>isManagerOf</code>
            <desc>Manages</desc>
            <help>Oversees the related party or service or administers the related collection (includes custodian role)</help>
         </item>

         <item>
            <code>isOwnedBy</code>
            <desc>Owned by</desc>
            <help>Legally belongs to the related party</help>
         </item>

         <item>
            <code>isOwnerOf</code>
            <desc>Owner of</desc>
            <help>Legally possesses the related activity, collection, service or group</help>
         </item>

      </any>

   </party>

   <service>
      <service>

         <item>
            <code>hasPart</code>
            <desc>Includes</desc>
            <help>Contains the related service</help>
         </item>
      </service>

      <collection>

         <item>
            <code>isSupportedBy</code>
            <desc>Supported by</desc>
            <help>Enables contribution and access to and use of the related collection (all services) (in technical terms, the collection supports the service)</help>
         </item>

         <item>
            <code>makesAvailable</code>
            <desc>Makes available</desc>
            <help>Discovery Services (Harvest, Search, Syndicate)</help>
         </item>

         <item>
            <code>produces</code>
            <desc>Produces</desc>
            <help>Creation Services (Create, Generate, Assemble, Transform output)</help>
         </item>

         <item>
            <code>presents</code>
            <desc>Presents</desc>
            <help>Creation Services (Report)</help>
         </item>

         <item>
            <code>operatesOn</code>
            <desc>Operates on</desc>
            <help>Creation Services (Transform input)</help>
         </item>

         <item>
            <code>addsValueTo</code>
            <desc>Adds value to</desc>
            <help>Metadata Services (Annotate, Classify)</help>
         </item>

      </collection>

      <party>

         <item>
            <code>isManagedBy</code>
            <desc>Managed by</desc>
            <help>Is overseen by the related party (includes custodian role)</help>
         </item>

         <item>
            <code>isOwnedBy</code>
            <desc>Owned by</desc>
            <help>Legally belongs to the related party</help>
         </item>

      </party>

      <any>

         <item>
            <code>hasAssociationWith</code>
            <desc>Associated with</desc>
            <help>Has an unspecified relationship with the related registry object</help>
         </item>

      </any>

   </service>

</list>
